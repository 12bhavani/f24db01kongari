extends layout

block content
  h1= title
  p #{title} page is running by Bhavani Kongari
  - var image_names = ["black.jpg", "brown.jpg", "grey.jpg", "orange.jpg", "white.jpg"];
  - for (let i = 0; i < 3; i++)
    - let index = Math.floor(Math.random() * 5);
    img(src=`/images/${image_names[index]}`, alt=`${image_names[index]}`, height="300", width="300")
